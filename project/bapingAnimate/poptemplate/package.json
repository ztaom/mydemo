{
  "name": "aft-ui-demo",
  "description": "互动营销项目",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:mtb/aft-ui-demo.git"
  },
  "author": "夜翎 <kaiqian.mkq@alibaba-inc.com>",
  "license": "MIT",
  "keywords": [
    "hudong",
    "fdilab",
    "aft",
    "javascript",
    "animation",
    "2d",
    "3d",
    "webgl",
    "canvas",
    "css2d",
    "css3d",
    "svg"
  ],
  "scripts": {
    "clear": "rm -rf app/dist",
    "upload1x": "image2cdn -p assets/1x/ -o assets/1x/ --cdnurlName url.json --base64Limit 1",
    "upload2x": "image2cdn -p assets/2x/ -o assets/2x/ --cdnurlName url.json --base64Limit 1",
    "upload": "npm run upload1x && npm run upload2x",
    "watch-upload": "gazeer -p 'assets/**/*.png' -p 'assets/**/*.jpg' -c 'npm run upload'",
    "build-dev": "webpack --config build/webpack.dev.config.js",
    "build-dist": "webpack --config build/webpack.dist.config.js",
    "minify": "uglifyjs --screw-ie8 -c -o app/dist/index.js -- app/dist/index.js",
    "inline": "inline-source --compress false --root ./app app/index.html > app/dist/index.html",
    "dev": "npm run clear && npm run upload && npm run build-dev -- --watch",
    "release-dev": "npm run clear && npm run build-dev && npm run inline",
    "release": "npm run clear && npm run build-dist && npm run minify && npm run inline",
    "node": "npm run build-dev && npm run inline"
  },
  "dependencies": {
    "@ali/aft.js": "^0.1.1",
    "@ali/browser-detect": "^3.0.2",
    "@ali/lib-bridge": "^2.5.6",
    "@ali/lib-env": "^1.9.9",
    "@ali/lib-goldlog": "^0.0.6",
    "@ali/lib-httpurl": "^2.0.2",
    "@ali/lib-mtop": "^2.3.5",
    "@ali/lib-poplayer": "^0.1.4",
    "@ali/lib-promise": "^3.1.3",
    "@ali/lib-windvane": "^2.1.7",
    "createjs-preloadjs": "^0.6.3"
  },
  "devDependencies": {
    "@ali/aft.js": "^0.1.2",
    "@ali/image2cdn": "^1.0.3",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.1",
    "babel-generator": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-latest": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "gazeer": "^0.1.1",
    "inline-source-cli": "^1.1.1",
    "json-beautify": "^1.0.1",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.1",
    "postcss-px2rem": "^0.3.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.2"
  },
  "main": "./app/dist/index.js"
}
