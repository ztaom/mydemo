{
  "name": "webpacktemplate",
  "version": "1.0.0",
  "description": "",
  "author": "阿虏 <xuerong@alibaba-inc.com>",
  "license": "ISC",
  "scripts": {
    "clear": "rm -rf dist",
    "inline": "inline-source --compress false --root ./index.html > ./dist/index.html",
    "build": "rimraf dist && webpack -p --env.config production",
    "build-dev": "webpack --config ./build/webpack.dev.config.js",
    "build-dist": "webpack --config ./build/webpack.dist.config.js",
    "devserver": "webpack-dev-server  --hot --inline",
    "dev": "npm run clear && tnpm run devserver && npm run build-dev -- --watch",
    "release-dev": "npm run clear && npm run build-dev",
    "release": "npm run clear && npm run build"
  },
  "dependencies": {
    "@ali/htmlone-core": "^1.0.3",
    "@ali/lib-env": "^1.9.9",
    "@ali/lib-windvane": "^2.1.8",
    "amfe-env": "^2.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.4",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-config-enough": "^0.2.5",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "postcss-loader": "^1.3.1",
    "react": "^15.5.4",
    "rimraf": "^2.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "main": "webpack.config.js"
}
