{
  "name": "@ali/aft.js",
  "version": "0.3.10",
  "description": "An Javascript Library of Animation Flow",
  "main": "dist/lite.js",
  "scripts": {
    "clear": "rm -rf dist/* browser/*",
    "lint": "eslint --fix --config .eslintrc src/*.js src/**/*.js test/specs/*.js test/specs/**/*.js",
    "hexo:init": "cd docs && tnpm i -g hexo-cli && tnpm i",
    "hexo:server": "hexo server --cwd docs",
    "hexo:build": "rm -rf docs/public && node docs/update.js && hexo g --cwd docs --config _config.publish.yml",
    "docs": " npm run hexo:build",
    "serve": "serve ./ -p 4001",
    "test:specs": "mocha --compilers js:babel-core/register test/specs/**/*Test.js",
    "test": "npm run test:specs",
    "cover:specs": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter dot test/specs/**/*Test.js",
    "cover:badge": "istanbul-cobertura-badger -e 85 -g 70 -r coverage/cobertura-coverage.xml -d . -b coverage",
    "precover": "rm -rf coverage/*",
    "cover": "npm run cover:specs",
    "ci": "npm run lint && npm run cover",
    "babel": "babel src --out-dir dist",
    "bundle-lite": "rollup -c build/rollup.lite.config.js",
    "bundle-render": "rollup -c build/rollup.render.config.js",
    "bundle-legacy": "rollup -c build/rollup.legacy.config.js",
    "bundle": "npm run bundle-lite && npm run bundle-render && npm run bundle-legacy",
    "browser-dev": "webpack --config build/webpack.dev.config.js",
    "browser": "webpack --optimize-minimize --config build/webpack.dist.config.js",
    "polyfill": "webpack --optimize-minimize --config build/webpack.polyfill.config.js",
    "dist": "npm run babel && npm run bundle && npm run browser-dev && npm run browser && npm run polyfill",
    "prerelease": "npm run clear && npm run ci && npm run dist && npm run docs && node build/prerelease.js",
    "release": "tnpm publish dist"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  },
  "homepage": "http://aft.alibaba-inc.com",
  "bugs": {
    "url": "http://gitlab.alibaba-inc.com/fdilab/aft.js/issues"
  },
  "keywords": [
    "javascript",
    "animation",
    "2d",
    "3d",
    "webgl",
    "canvas",
    "css2d",
    "css3d",
    "svg"
  ],
  "private": false,
  "author": "zhuxun.jb <zhuxun.jb@alibaba-inc.com>",
  "maintainers": [
    "kaiqian.mkq <kaiqian.mkq@alibaba-inc.com>",
    "shangzuo.zm <shangzuo.zm@alibaba-inc.com>",
    "huajian.hhj <huajian.hhj@alibaba-inc.com>"
  ],
  "contributors": [
    "jingyi.zl <jingyi.zl@alibaba-inc.com>",
    "weihua.lwh <weihua.lwh@alibaba-inc.com>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-check-es2015-constants": "~6.22.0",
    "babel-plugin-transform-es2015-arrow-functions": "~6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "~6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "~6.24.1",
    "babel-plugin-transform-es2015-classes": "~6.24.1",
    "babel-plugin-transform-es2015-computed-properties": "~6.24.1",
    "babel-plugin-transform-es2015-destructuring": "~6.23.0",
    "babel-plugin-transform-es2015-for-of": "~6.23.0",
    "babel-plugin-transform-es2015-function-name": "~6.24.1",
    "babel-plugin-transform-es2015-literals": "~6.22.0",
    "babel-plugin-transform-es2015-object-super": "~6.24.1",
    "babel-plugin-transform-es2015-parameters": "~6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "~6.24.1",
    "babel-plugin-transform-es2015-spread": "~6.22.0",
    "babel-plugin-transform-es2015-sticky-regex": "~6.24.1",
    "babel-plugin-transform-es2015-template-literals": "~6.22.0",
    "babel-plugin-transform-es2015-typeof-symbol": "~6.23.0",
    "babel-plugin-transform-es2015-unicode-regex": "~6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^3.2.1",
    "eslint": "^2.13.0",
    "eslint-plugin-babel": "^3.3.0",
    "global-object": "^1.0.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "istanbul-cobertura-badger": "^1.3.0",
    "mocha": "^3.0.1",
    "mocha-generators": "~1.2.0",
    "mocha-jsdom": "^1.1.0",
    "promise-thunk": "^0.1.17",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^1.7.3",
    "rollup-watch": "^2.5.0",
    "serve": "^1.4.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "thunks": "^4.7.6",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "performance-now": "^2.1.0",
    "babel-runtime": "^6.11.6",
    "css-transform-to-mat4": "^1.0.4",
    "event-emitter": "^0.3.4",
    "gl-mat4": "^1.1.4",
    "gl-vec3": "^1.0.3",
    "lodash": "^4.16.0",
    "pure-color": "^1.2.0"
  }
}
